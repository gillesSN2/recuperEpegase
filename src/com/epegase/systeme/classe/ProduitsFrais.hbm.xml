<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.epegase.systeme.classe">
  <class name="ProduitsFrais" table="cmm_produits_frais">
    <id column="profrs_id" name="profrsId" type="long" unsaved-value="null">
      <generator class="native"/>
    </id>
    <property name="profrsOrdre" type="integer">
      <column default="0" length="3" name="profrs_ordre">
        <comment>ordre des reponses</comment>
      </column>
    </property>
    <property name="profrsFeuille" type="string">
      <column length="100" name="profrs_feuille">
        <comment>nom feuille</comment>
      </column>
    </property>
    <property name="profrsNum" type="string">
      <column length="20" name="profrs_num">
        <comment>numero de calcul</comment>
      </column>
    </property>
    <property name="profrsType" type="int">
      <column default="0" name="profrs_type">
        <comment>type de la feuille 0=sur produit 1=sur cotation 2=sur commande 3=parametre fournisseur</comment>
      </column>
    </property>
    <property name="profrsCategorie" type="int">
      <column default="0" name="profrs_categorie">
        <comment>categorie 0=ns 1=douane 2=debours 3=prestation 4=autre</comment>
      </column>
    </property>
    <property name="profrsMode" type="string">
      <column length="50" name="profrs_mode">
        <comment>mode importation ex contener, groupage, type de produit</comment>
      </column>
    </property>
    <property name="profrsNature" type="int">
      <column default="0" name="profrs_nature">
        <comment>nature importation 0=avion 1=bateau 2=express 3=route</comment>
      </column>
    </property>
    <property name="profrsReponse" type="int">
      <column default="0" name="profrs_reponse">
        <comment>reponse 0=ns 1=non 2=oui</comment>
      </column>
    </property>
    <property name="profrsDate" type="date">
      <column name="profrs_date">
        <comment>date de creation</comment>
      </column>
    </property>
    <property name="profrsRubrique" type="string">
      <column length="50" name="profrs_rubrique">
        <comment>code rubrique associe</comment>
      </column>
    </property>
    <property name="profrsCode" type="string">

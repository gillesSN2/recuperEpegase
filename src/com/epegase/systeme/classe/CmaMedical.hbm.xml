<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.epegase.systeme.classe">
  <class name="CmaMedical" table="med_cma">
    <id column="cma_id" name="cmaId" type="long" unsaved-value="null">
      <generator class="native"/>
    </id>
    <property name="cmaCode" type="string">
      <column length="20" name="cma_code">
        <comment>code cma</comment>
      </column>
    </property>
    <property name="cmaLibelleFr" type="string">
      <column length="500" name="cma_libelle_FR">
        <comment>libelle cma FR</comment>
      </column>
    </property>
    <property name="cmaLibelleUk" type="string">
      <column length="500" name="cma_libelle_UK">
        <comment>libelle cma UK</comment>
      </column>
    </property>
    <property name="cmaLibelleSp" type="string">
      <column length="500" name="cma_libelle_SP">
        <comment>libelle cma UK</comment>
      </column>
    </property>
    <property name="cmaCma" type="integer">
      <column default="0" length="1" name="cma_cma">
        <comment>cma</comment>
      </column>
    </property>
    <property name="cmaCmas" type="integer">
      <column default="0" length="1" name="cma_cmas">
        <comment>cmas</comment>
      </column>
    </property>
    <property name="cmaCmasnt" type="integer">
      <column default="0" length="1" name="cma_cmasnt">
        <comment>cmasnt</comment>
      </column>
    </property>
  </class>
</hibernate-mapping>

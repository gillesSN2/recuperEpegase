      <column length="20" name="ccam_det_code">
        <comment>code ccam</comment>
      </column>
    </property>
    <property name="ccamDetLibFr" type="string">
      <column length="500" name="ccam_det_lib_FR"/>
    </property>
    <property name="ccamDetLibUk" type="string">
      <column length="500" name="ccam_det_lib_UK"/>
    </property>
    <property name="ccamDetLibSp" type="string">
      <column length="500" name="ccam_det_lib_SP"/>
    </property>
    <property name="ccamDetActivite" type="string">
      <column length="20" name="ccam_det_activite">
        <comment>code activite</comment>
      </column>
    </property>
    <property name="ccamDetPhase" type="string">
      <column length="50" name="ccam_det_phase"/>

    <property name="ccamFamLibUk" type="string">
      <column length="500" name="ccam_fam_lib_UK"/>
    </property>
    <property name="ccamFamLibSp" type="string">
      <column length="500" name="ccam_fam_lib_SP"/>
    </property>
    <property name="ccamSfamCode" type="string">
      <column length="20" name="ccam_sfam_code">
        <comment>code sous famille</comment>
      </column>
    </property>
    <property name="ccamSfamLibFr" type="string">
      <column length="500" name="ccam_sfam_lib_FR"/>
    </property>
    <property name="ccamSfamLibUk" type="string">
      <column length="500" name="ccam_sfam_lib_UK"/>
    </property>
    <property name="ccamSfamLibSp" type="string">
      <column length="500" name="ccam_sfam_lib_SP"/>
    </property>
    <property name="ccamSsfamCode" type="string">
      <column length="20" name="ccam_ssfam_code">
        <comment>code sous sous famille</comment>
      </column>
    </property>
    <property name="ccamSsfamLibFr" type="string">
      <column length="500" name="ccam_ssfam_lib_FR"/>
    </property>
    <property name="ccamSsfamLibUk" type="string">
      <column length="500" name="ccam_ssfam_lib_UK"/>
    </property>
    <property name="ccamSsfamLibSp" type="string">
      <column length="500" name="ccam_ssfam_lib_SP"/>
    </property>
    <property name="ccamSssfamCode" type="string">
      <column length="20" name="ccam_sssfam_code">
        <comment>code sous sous sous famille</comment>
      </column>
    </property>
    <property name="ccamSssfamLibFr" type="string">
      <column length="500" name="ccam_sssfam_lib_FR"/>
    </property>
    <property name="ccamSssfamLibUk" type="string">
      <column length="500" name="ccam_sssfam_lib_UK"/>

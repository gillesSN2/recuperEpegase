<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.epegase.systeme.classe">
  <class name="PegDouanesSection" table="peg_douanes_section">
    <id column="dousec_id" name="dousecId" type="long" unsaved-value="null">
      <generator class="native"/>
    </id>
    <property name="dousecZone" type="string">
      <column length="30" name="dousec_zone">
        <comment>code zone</comment>
      </column>
    </property>
    <property name="dousecCode" type="string">
      <column length="10" name="dousec_code">
        <comment>code section</comment>
      </column>
    </property>
    <property name="dousecLibFR" type="string">
      <column length="500" name="dousec_lib_FR">
        <comment>libelle FR</comment>
      </column>
    </property>
    <property name="dousecLibUK" type="string">
      <column length="500" name="dousec_lib_UK">
        <comment>libelle UK</comment>
      </column>
    </property>
    <property name="dousecLibSP" type="string">
      <column length="500" name="dousec_lib_SP">
        <comment>libelle SP</comment>
      </column>
    </property>
  </class>
</hibernate-mapping>

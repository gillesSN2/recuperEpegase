<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.epegase.systeme.classe">
  <class name="BesoinLigneVentes" table="vte_besoin_ligne">
    <id column="beslig_id" name="besligId" type="long" unsaved-value="null">
      <generator class="native"/>
    </id>
    <property name="besligCode" type="string">
      <column length="20" name="beslig_code">
        <comment>code produit</comment>
      </column>
    </property>
    <property name="besligFamille" type="string">
      <column length="121" name="beslig_famille">
        <comment>famille vente</comment>
      </column>
    </property>
    <property name="besligLibelle" type="text">
      <column length="500" name="beslig_libelle">
        <comment>libelle produit</comment>
      </column>
    </property>
    <property name="besligReference" type="string">
      <column length="20" name="beslig_reference">
        <comment>reference produit</comment>
      </column>
    </property>
    <property name="besligTaxe" type="string">
      <column length="20" name="beslig_taxe">
        <comment>code taxe</comment>
      </column>
    </property>
    <property name="besligTauxTaxe" type="float">
      <column default="0" name="beslig_taux_taxe">
        <comment>taux de taxe</comment>
      </column>
    </property>
    <property name="besligUnite" type="string">
      <column length="20" name="beslig_unite">
        <comment>unite produit</comment>
      </column>
    </property>
    <property name="besligCondition" type="string">
      <column length="121" name="beslig_condition">
        <comment>conditionnement produit</comment>
      </column>
    </property>
    <property name="besligDescription" type="string">
      <column length="20" name="beslig_description">
        <comment>description conditionnement produit</comment>
      </column>
    </property>
    <property name="besligEchelle" type="integer">
      <column default="0" name="beslig_echelle">
        <comment>echelle de la ligne</comment>
      </column>
    </property>
    <property name="besligQte" type="float">
      <column default="0" name="beslig_qte">
        <comment>quantite</comment>
      </column>
    </property>
    <property name="besligLong" type="float">
      <column default="0" name="beslig_long">
        <comment>longueur</comment>
      </column>
    </property>
    <property name="besligLarg" type="float">
      <column default="0" name="beslig_larg">
        <comment>largeur</comment>
      </column>
    </property>
    <property name="besligHaut" type="float">
      <column default="0" name="beslig_haut">
        <comment>hauteur</comment>
      </column>
    </property>
    <property name="besligDiam" type="float">
      <column default="0" name="beslig_diam">
        <comment>diametre</comment>
      </column>
    </property>
    <property name="besligNb" type="float">
      <column default="0" name="beslig_nb">
        <comment>nombre de piece</comment>
      </column>
    </property>
    <property name="besligPoidsNet" type="float">
      <column default="0" name="beslig_poidsNet">
        <comment>poids net</comment>
      </column>
    </property>
    <property name="besligPoidsBrut" type="float">
      <column default="0" name="beslig_poidsBrut">
        <comment>poids brut</comment>
      </column>
    </property>
    <property name="besligVolume" type="float">
      <column default="0" name="beslig_volume">
        <comment>volume</comment>
      </column>
    </property>
    <property name="besligQteUtil" type="float">
      <column default="0" name="beslig_qte_util">
        <comment>quantite utilisee (valable pour le stock)</comment>
      </column>
    </property>
    <property name="besligStock" type="integer">
      <column default="0" name="beslig_stock">
        <comment>mode de gestion du stock</comment>
      </column>
    </property>
    <property name="besligDepot" type="string">
      <column length="20" name="beslig_depot">
        <comment>code depot</comment>
      </column>
    </property>
    <property name="besligDevise" type="string">
      <column length="3" name="beslig_devise">
        <comment>code devise</comment>
      </column>
    </property>
    <property name="besligPu" type="double">
      <column default="0" name="beslig_pu">
        <comment>prix unitaire HT avant remise</comment>
      </column>
    </property>
    <property name="besligTauxRemise" type="float">
      <column default="0" name="beslig_taux_remise">
        <comment>taux de remise</comment>
      </column>
    </property>
    <property name="besligRabais" type="double">
      <column default="0" name="beslig_rabais">
        <comment>valeur du rabais</comment>
      </column>
    </property>
    <property name="besligPuRem" type="double">
      <column default="0" name="beslig_pu_rem">
        <comment>prix unitaire Ht apres remise</comment>
      </column>
    </property>
    <property name="besligPuTtc" type="double">
      <column default="0" name="beslig_pu_ttc">
        <comment>prix unitaire Ttc avant remise</comment>
      </column>
    </property>
    <property name="besligPuRemTtc" type="double">
      <column default="0" name="beslig_pu_rem_ttc">
        <comment>prix unitaire Ttc apres remise</comment>
      </column>
    </property>
    <property name="besligPt" type="double">
      <column default="0" name="beslig_pt">
        <comment>prix total ht</comment>
      </column>
    </property>
    <property name="besligTva" type="double">
      <column default="0" name="beslig_tva">
        <comment>total taxe</comment>
      </column>
    </property>
    <property name="besligTc" type="double">
      <column default="0" name="beslig_tc">
        <comment>total taxe complementaire</comment>
      </column>
    </property>
    <property name="besligTtc" type="double">
      <column default="0" name="beslig_ttc">
        <comment>total ttc</comment>
      </column>
    </property>
    <property name="besligPump" type="double">
      <column default="0" name="beslig_pump">
        <comment>prix unitaire moyen pondere unitaire</comment>
      </column>
    </property>
    <property name="besligGroupe" type="string">
      <column length="20" name="beslig_groupe">
        <comment>code du groupe</comment>
      </column>
    </property>
    <property name="besligModeGroupe" type="integer">
      <column default="0" name="beslig_mode_groupe">
        <comment>0=produit simple 1=groupe visible 2=groupe invisible 3=forfait 4=calcul auto. 5=generique</comment>
      </column>
    </property>
    <many-to-one class="com.epegase.systeme.classe.BesoinEnteteVentes" column="bes_id" lazy="false" name="besoinEnteteVentes" not-null="true"/>
  </class>
</hibernate-mapping>

<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.epegase.systeme.classe">
  <class name="CimMedical" table="med_cim">
    <id column="cim_id" name="cimId" type="long" unsaved-value="null">
      <generator class="native"/>
    </id>
    <property name="cimCodeCmd" type="string">
      <column length="20" name="cim_code_cmd">
        <comment>code cmd</comment>
      </column>
    </property>
    <property name="cimLibCmd" type="string">
      <column length="500" name="cim_lib_cmd">
        <comment>libelle cmd</comment>
      </column>
    </property>
    <property name="cimCode" type="string">
      <column length="20" name="cim_code">
        <comment>code cim</comment>
      </column>
    </property>
    <property name="cimLibelleFR" type="string">
      <column length="500" name="cim_libelle_FR">
        <comment>libelle cim FR</comment>
      </column>
    </property>
    <property name="cimLibelleUK" type="string">
      <column length="500" name="cim_libelle_UK">
        <comment>libelle cim UK</comment>
      </column>
    </property>
    <property name="cimLibelleSP" type="string">
      <column length="500" name="cim_libelle_SP">
        <comment>libelle cim SP</comment>
      </column>
    </property>
  </class>
</hibernate-mapping>

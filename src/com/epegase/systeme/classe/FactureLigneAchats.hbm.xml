<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.epegase.systeme.classe">
  <class name="FactureLigneAchats" table="ach_facture_ligne">
    <id column="fcflig_id" name="fcfligId" type="long" unsaved-value="null">
      <generator class="increment"/>
    </id>
    <property name="fcfligIdDa" type="long">
      <column default="0" length="15" name="fcflig_id_da">
        <comment>id de la ligne de da</comment>
      </column>
    </property>
    <property name="fcfligIdCot" type="long">
      <column default="0" length="15" name="fcflig_id_cot">
        <comment>id de la ligne de cotation</comment>
      </column>
    </property>
    <property name="fcfligIdCmd" type="long">
      <column default="0" length="15" name="fcflig_id_cmd">
        <comment>id de la ligne de commande</comment>
      </column>
    </property>
    <property name="fcfligIdRec" type="long">
      <column default="0" length="15" name="fcflig_id_rec">
        <comment>id de la ligne de reception</comment>
      </column>
    </property>
    <property name="fcfligCode" type="string">
      <column length="20" name="fcflig_code">
        <comment>code produit</comment>
      </column>
    </property>
    <property name="fcfligFamille" type="string">
      <column length="121" name="fcflig_famille">
        <comment>famille vente</comment>
      </column>
    </property>
    <property name="fcfligLibelle" type="text">
      <column name="fcflig_libelle">
        <comment>libelle produit</comment>
      </column>
    </property>
    <property name="fcfligLibelleFournisseur" type="string">
      <column length="100" name="fcflig_libelle_fournisseur">
        <comment>libelle produit</comment>
      </column>
    </property>
    <property name="fcfligComplement" type="text">
      <column length="500" name="fcflig_complement">
        <comment>descriptif complementaire</comment>
      </column>
    </property>
    <property name="fcfligReference" type="string">
      <column length="20" name="fcflig_reference">
        <comment>reference produit</comment>
      </column>
    </property>
    <property name="fcfligTaxe" type="string">
      <column length="20" name="fcflig_taxe">
        <comment>code taxe</comment>
      </column>
    </property>
    <property name="fcfligTauxTaxe" type="float">
      <column default="0" name="fcflig_taux_taxe">
        <comment>taux de taxe</comment>
      </column>
    </property>
    <property name="fcfligUnite" type="string">
      <column length="30" name="fcflig_unite">
        <comment>unite produit</comment>
      </column>
    </property>
    <property name="fcfligCondition" type="string">
      <column length="121" name="fcflig_condition">
        <comment>conditionnement produit</comment>
      </column>
    </property>
    <property name="fcfligDescription" type="string">
      <column length="20" name="fcflig_description">
        <comment>description conditionnement produit</comment>
      </column>
    </property>
    <property name="fcfligQte" type="float">
      <column default="0" name="fcflig_qte">
        <comment>quantite</comment>
      </column>
    </property>
    <property name="fcfligLong" type="float">
      <column default="0" name="fcflig_long">
        <comment>longueur</comment>
      </column>
    </property>
    <property name="fcfligLarg" type="float">
      <column default="0" name="fcflig_larg">
        <comment>largeur</comment>
      </column>
    </property>
    <property name="fcfligHaut" type="float">
      <column default="0" name="fcflig_haut">
        <comment>hauteur</comment>
      </column>
    </property>
    <property name="fcfligDiam" type="float">
      <column default="0" name="fcflig_diam">
        <comment>diametre</comment>
      </column>
    </property>
    <property name="fcfligNb" type="float">
      <column default="0" name="fcflig_nb">
        <comment>nombre de piece</comment>
      </column>
    </property>
    <property name="fcfligPoidsNet" type="float">
      <column default="0" name="fcflig_poidsNet">
        <comment>poids net</comment>
      </column>
    </property>
    <property name="fcfligPoidsBrut" type="float">
      <column default="0" name="fcflig_poidsBrut">
        <comment>poids brut</comment>
      </column>
    </property>
    <property name="fcfligVolume" type="float">
      <column default="0" name="fcflig_volume">
        <comment>volume</comment>
      </column>
    </property>
    <property name="fcfligQteUtil" type="float">
      <column default="0" name="fcflig_qte_util">
        <comment>quantite utilisee (valable pour le stock)</comment>
      </column>
    </property>
    <property name="fcfligDepot" type="string">
      <column length="20" name="fcflig_depot">
        <comment>code depot</comment>
      </column>
    </property>
    <property name="fcfligQteStock" type="float">
      <column default="0" name="fcflig_qte_stock">
        <comment>quantite en stock du depot</comment>
      </column>
    </property>
    <property name="fcfligEchelle" type="integer">
      <column default="0" name="fcflig_echelle">
        <comment>echelle de la ligne</comment>
      </column>
    </property>
    <property name="fcfligDevise" type="string">
      <column length="3" name="fcflig_devise">
        <comment>code devise</comment>
      </column>
    </property>
    <property name="fcfligPu" type="double">
      <column default="0" name="fcflig_pu">
        <comment>prix unitaire</comment>
      </column>
    </property>
    <property name="fcfligTauxRemise" type="float">
      <column default="0" name="fcflig_taux_remise">
        <comment>taux de remise</comment>
      </column>
    </property>
    <property name="fcfligRabais" type="double">
      <column default="0" name="fcflig_rabais">
        <comment>valeur du rabais</comment>
      </column>
    </property>
    <property name="fcfligPuRem" type="double">
      <column default="0" name="fcflig_pu_rem">
        <comment>prix unitaire apres remise</comment>
      </column>
    </property>
    <property name="fcfligPtDev" type="double">
      <column default="0" name="fcflig_pt_dev">
        <comment>prix total ht en devise</comment>
      </column>
    </property>
    <property name="fcfligPt" type="double">
      <column default="0" name="fcflig_pt">
        <comment>prix total ht</comment>
      </column>
    </property>
    <property name="fcfligTva" type="double">
      <column default="0" name="fcflig_tva">
        <comment>total taxe</comment>
      </column>
    </property>
    <property name="fcfligTc" type="double">
      <column default="0" name="fcflig_tc">
        <comment>total taxe complementaire</comment>
      </column>
    </property>
    <property name="fcfligTtc" type="double">
      <column default="0" name="fcflig_ttc">
        <comment>total ttc</comment>
      </column>
    </property>
    <property name="fcfligPr" type="double">
      <column default="0" name="fcflig_pr">
        <comment>prix de revient</comment>
      </column>
    </property>
    <property name="fcfligPump" type="double">
      <column default="0" name="fcflig_pump">
        <comment>prix unitaire moyen pondere</comment>
      </column>
    </property>
    <many-to-one class="com.epegase.systeme.classe.FactureEnteteAchats" column="fcf_id" lazy="false" name="factureEnteteAchats" not-null="true"/>
  </class>
</hibernate-mapping>

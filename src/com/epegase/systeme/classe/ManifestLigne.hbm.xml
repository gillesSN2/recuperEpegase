      <column default="null" name="vtelv_dateCreat">
        <comment>date de creation</comment>
      </column>
    </property>
    <property name="vtelvDateModif" type="timestamp">
      <column default="null" name="vtelv_dateModif">
        <comment>date de modification</comment>
      </column>
    </property>
    <property name="vtelvIdUserCreat" type="long">

    <property name="vtelvValeurAssuree" type="integer">
      <column default="0" name="vtelv_valeurAssuree">
        <comment>montant valeur assure</comment>
      </column>
    </property>
    <property name="vtelvNumManifest" type="string">
      <column length="20" name="vtelv_numManifest">
        <comment>numero manifeste</comment>
      </column>
    </property>
    <property name="vtelvNbreColisExp" type="integer">

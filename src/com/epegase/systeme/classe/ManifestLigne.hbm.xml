    <property name="vtelvExoTc" type="integer">
      <column default="0" name="vtelv_exo_tc">
        <comment>0= avec tc 1=sans tc</comment>
      </column>
    </property>
    <property name="vtelvModeClient" type="integer">
      <column default="0" name="vtelv_mode_clioent">
        <comment>0=pas de client 1=port paye (expediteur) 2=port du (destinataire)</comment>
      </column>
    </property>
    <property name="vtelvModeFacture" type="integer">
      <column default="0" name="vtelv_mode_facture">
        <comment>0=facture produit 1=facture sur devis</comment>
      </column>
    </property>
    <property name="vtelvModeAssuree" type="integer">
      <column default="0" name="vtelv_mode_assuree">
        <comment>0=sans assurance 1=perte ou vol 2=tous risques</comment>
      </column>
    </property>
    <property name="vtelvModeGroupe" type="integer">
      <column default="0" name="vtelv_mode_groupe">
        <comment>0=facture normale 1=facture groupe</comment>
      </column>
    </property>

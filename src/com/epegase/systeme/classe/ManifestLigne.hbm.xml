    <property name="vtelvIdUserModif" type="long">
      <column default="0" name="vtelv_idUser_modif">
        <comment>id user modification</comment>
      </column>
    </property>
    <property name="vtelvNum" type="string">
      <column length="20" name="vtelv_num">
        <comment>numero lettre voiture</comment>
      </column>
    </property>
    <property name="vtelvOrdre" type="integer">
      <column default="0" name="vtelv_ordre">

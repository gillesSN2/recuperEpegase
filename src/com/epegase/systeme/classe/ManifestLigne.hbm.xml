    <property name="vtelvAdresseExp" type="string">
      <column length="100" name="vtelv_adresseExp">
        <comment>adresse depart</comment>
      </column>
    </property>
    <property name="vtelvContactExp" type="string">
      <column length="100" name="vtelv_contactExp">
        <comment>contact depart</comment>
      </column>
    </property>
    <property name="vtelvMatChauffeurDest" type="string">
      <column length="20" name="vtelv_matChauffeurDest">
        <comment>matricule du chauffeur arrivee</comment>
      </column>
    </property>
    <property name="vtelvNomPrenomChauffeurDest" type="string">
      <column length="200" name="vtelv_nomPrenomChauffeurDest">
        <comment>nom et prenom du chauffeur arrivee</comment>
      </column>
    </property>
    <property name="vtelvKmsDest" type="double">
      <column default="0" name="vtelv_kmsDest">
        <comment>kilometrage destination</comment>
      </column>
    </property>
    <property name="vtelvKmsReel" type="double">

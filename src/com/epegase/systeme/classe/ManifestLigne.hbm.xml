    <property name="vtelvImmaRemorque2" type="string">
      <column length="100" name="vtelv_immaRemorque2">
        <comment>immatriculation remorque 2</comment>
      </column>
    </property>
    <property name="vtelvMatChauffeurExp" type="string">
      <column length="20" name="vtelv_matChauffeurExp">
        <comment>matricule du chauffeur depart</comment>
      </column>

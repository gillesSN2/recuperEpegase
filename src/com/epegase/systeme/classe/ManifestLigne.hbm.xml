      <column length="20" name="vtelv_num_devis">
        <comment>numero de devis</comment>
      </column>
    </property>
    <property name="vtelvNumFacture" type="string">
      <column length="20" name="vtelv_num_facture">
        <comment>numero de facture</comment>
      </column>
    </property>

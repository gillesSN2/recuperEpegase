<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.epegase.systeme.classe">
  <class name="ReceptionLotAchats" table="ach_reception_lot">
    <id column="reclot_id" name="reclotId" type="long" unsaved-value="null">
      <generator class="native"/>
    </id>
    <property name="reclotIdLigne" type="long">
      <column default="0" name="reclot_id_ligne">
        <comment>id ligne de reception</comment>
      </column>
    </property>
    <property name="reclotNum" type="string">
      <column default="null" length="20" name="reclot_num">
        <comment>numero de reception</comment>
      </column>
    </property>
    <property name="reclotDateAchat" type="date">
      <column default="null" name="reclot_date_achat">
        <comment>date achat</comment>
      </column>
    </property>
    <property name="reclotIdLigneFab" type="long">
      <column default="0" name="reclot_id_ligne_fab">
        <comment>id ligne de fabrication</comment>
      </column>
    </property>
    <property name="reclotNumFab" type="string">
      <column default="null" length="20" name="reclot_num_fab">
        <comment>numero de fabrication</comment>
      </column>
    </property>
    <property name="reclotDateFab" type="date">
      <column default="null" name="reclot_date_fab">
        <comment>date fabrication</comment>
      </column>
    </property>
    <property name="reclotDateValable" type="date">
      <column default="null" name="reclot_date_valable">
        <comment>valable jusqu  au</comment>
      </column>
    </property>
    <property name="reclotQte" type="float">
      <column default="0" name="reclot_qte">
        <comment>quantite du lot</comment>
      </column>
    </property>
    <property name="reclotQteUtil" type="float">
      <column default="0" name="reclot_qte_util">
        <comment>quantite pour le stock</comment>
      </column>
    </property>
    <property name="reclotQteConso" type="float">
      <column default="0" name="reclot_qte_conso">
        <comment>quantite consommee</comment>
      </column>
    </property>
    <property name="reclotQteUtilConso" type="float">
      <column default="0" name="reclot_qte_util_conso">
        <comment>quantite consomme pour le stock</comment>
      </column>
    </property>
    <property name="reclotCode" type="string">
      <column default="null" length="20" name="reclot_code">
        <comment>code produit</comment>
      </column>
    </property>
    <property name="reclotDepot" type="string">
      <column default="null" length="10" name="reclot_depot">
        <comment>code depot</comment>
      </column>
    </property>
    <property name="reclotPr" type="double">
      <column default="0" name="reclot_pr">
        <comment>prix de revient</comment>
      </column>
    </property>
    <property name="reclotLong" type="float">
      <column default="0" name="reclot_long">
        <comment>longueur</comment>
      </column>
    </property>
    <property name="reclotLarg" type="float">
      <column default="0" name="reclot_larg">
        <comment>largeur</comment>
      </column>
    </property>
    <property name="reclotHaut" type="float">
      <column default="0" name="reclot_haut">
        <comment>hauteur</comment>
      </column>
    </property>
    <property name="reclotDiam" type="float">
      <column default="0" name="reclot_diam">
        <comment>diametre</comment>
      </column>
    </property>
    <property name="reclotNb" type="float">
      <column default="0" name="reclot_nb">
        <comment>nombre de piece</comment>
      </column>
    </property>
    <property name="reclotPoidsNet" type="float">
      <column default="0" name="reclot_poidsNet">
        <comment>poids net</comment>
      </column>
    </property>
    <property name="reclotPoidsTare" type="float">
      <column default="0" name="reclot_poidsTare">
        <comment>poids tare</comment>
      </column>
    </property>
    <property name="reclotPoidsBrut" type="float">
      <column default="0" name="reclot_poidsBrut">
        <comment>poids brut</comment>
      </column>
    </property>
    <property name="reclotNumero" type="string">
      <column default="null" length="100" name="reclot_numero">
        <comment>numero du lot</comment>
      </column>
    </property>
  </class>
</hibernate-mapping>

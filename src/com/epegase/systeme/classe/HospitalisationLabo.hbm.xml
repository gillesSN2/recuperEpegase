<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.epegase.systeme.classe">
  <class name="HospitalisationLabo" table="med_hospitalisation_labo">
    <id column="hoslab_id" name="hoslabId" type="long" unsaved-value="null">
      <generator class="native"/>
    </id>
    <property name="hoslabDateCreat" type="timestamp">
      <column default="null" name="hoslab_date_creat">
        <comment>date de creation</comment>
      </column>
    </property>
    <property name="hoslabDateModif" type="timestamp">
      <column default="null" name="hoslab_date_modif">
        <comment>date de modification</comment>
      </column>
    </property>
    <property name="hoslabUserCreat" type="long">
      <column default="0" name="hoslab_user_creat">
        <comment>user de creation</comment>
      </column>
    </property>
    <property name="hoslabUserModif" type="long">
      <column default="0" name="hoslab_user_modif">
        <comment>user de modification</comment>
      </column>
    </property>
    <property name="hoslabIdMedecin" type="long">
      <column default="0" name="hoslab_id_medecin">
        <comment>id medecin</comment>
      </column>
    </property>
    <property name="hoslabMedecin" type="string">
      <column length="100" name="hoslab_medecin">
        <comment>nom medecin</comment>
      </column>
    </property>
    <property name="hoslabService" type="string">
      <column length="121" name="hoslab_service">
        <comment>service</comment>
      </column>
    </property>
    <property name="hoslabLaboratoire" type="string">
      <column length="121" name="hoslab_laboratoire">
        <comment>laboratoire</comment>
      </column>
    </property>
    <property name="hoslabIdLaboratoire" type="long">
      <column default="0" name="hoslab_id_laboratoire">
        <comment>id laboratoire</comment>
      </column>
    </property>
    <property name="hoslabProduit" type="string">
      <column length="20" name="hoslab_produit">
        <comment>code produit</comment>
      </column>
    </property>
    <property name="hoslabLibelle" type="text">
      <column name="hoslab_libelle">
        <comment>libelle produit</comment>
      </column>
    </property>
    <property name="hoslabLettre" type="string">
      <column length="10" name="hoslab_lettre">
        <comment>code lettre</comment>
      </column>
    </property>
    <property name="hoslabNb" type="float">
      <column default="0" name="hoslab_nb">
        <comment>nombre lettre</comment>
      </column>
    </property>
    <property name="hoslabNbCnamgs" type="float">
      <column default="0" name="hoslab_nb_cnamgs">
        <comment>nombre lettre cnamgs</comment>
      </column>
    </property>
    <property name="hoslabValeur" type="double">
      <column default="0" name="hoslab_valeur">
        <comment>valeur lettre</comment>
      </column>
    </property>
    <property name="hoslabValeurCnamgs" type="double">
      <column default="0" name="hoslab_valeur_cnamgs">
        <comment>valeur lettre cnamgs</comment>
      </column>
    </property>
    <property name="hoslabCoef" type="float">
      <column default="0" name="hoslab_coef">
        <comment>coefficient</comment>
      </column>
    </property>
    <property name="hoslabPu" type="double">
      <column default="0" name="hoslab_pu">
        <comment>prix unitaire</comment>
      </column>
    </property>
    <property name="hoslabPuCnamgs" type="double">
      <column default="0" name="hoslab_pu_cnamgs">
        <comment>prix unitaire cnamgs</comment>
      </column>
    </property>
    <property name="hoslabPuAssurance" type="double">
      <column default="0" name="hoslab_pu_assurance">
        <comment>prix unitaire assurance</comment>
      </column>
    </property>
    <property name="hoslabCodeTva" type="string">
      <column length="20" name="hoslab_code_tva">
        <comment>code de tva</comment>
      </column>
    </property>
    <property name="hoslabTauxTva" type="float">
      <column default="0" name="hoslab_taux_tva">
        <comment>taux de tva</comment>
      </column>
    </property>
    <property name="hoslabRemise" type="float">
      <column default="0" name="hoslab_remise">
        <comment>%remise</comment>
      </column>
    </property>
    <property name="hoslabRabais" type="double">
      <column default="0" name="hoslab_rabais">
        <comment>rabais</comment>
      </column>
    </property>
    <property name="hoslabPuRem" type="double">
      <column default="0" name="hoslab_pu_rem">
        <comment>prix apres remise</comment>
      </column>
    </property>
    <property name="hoslabQte" type="float">
      <column default="0" name="hoslab_qte">
        <comment>quantite</comment>
      </column>
    </property>
    <property name="hoslabPatientHt" type="double">
      <column default="0" name="hoslab_patient_ht">
        <comment>part ht patient</comment>
      </column>
    </property>
    <property name="hoslabPatientTaxe" type="double">
      <column default="0" name="hoslab_patient_taxe">
        <comment>part taxe patient</comment>
      </column>
    </property>
    <property name="hoslabSocieteHt" type="double">
      <column default="0" name="hoslab_societe_ht">
        <comment>part ht societe</comment>
      </column>
    </property>
    <property name="hoslabSocieteTaxe" type="double">
      <column default="0" name="hoslab_societe_taxe">
        <comment>part taxe societe</comment>
      </column>
    </property>
    <property name="hoslabAssuranceHt" type="double">
      <column default="0" name="hoslab_assurance_ht">
        <comment>part ht assurance</comment>
      </column>
    </property>
    <property name="hoslabAssuranceTaxe" type="double">
      <column default="0" name="hoslab_assurance_taxe">
        <comment>part taxe assurance</comment>
      </column>
    </property>
    <property name="hoslabComplementaireHt" type="double">
      <column default="0" name="hoslab_complementaire_ht">
        <comment>part ht comllementaire</comment>
      </column>
    </property>
    <property name="hoslabComplementaireTaxe" type="double">
      <column default="0" name="hoslab_complementaire_taxe">
        <comment>part taxe complementaire</comment>
      </column>
    </property>
    <property name="hoslabTotal" type="double">
      <column default="0" name="hoslab_total">
        <comment>total ht</comment>
      </column>
    </property>
    <property name="hoslabTaxe" type="double">
      <column default="0" name="hoslab_taxe">
        <comment>total taxe</comment>
      </column>
    </property>
    <property name="hoslabIdSejour" type="long">
      <column default="0" name="hoslab_id_sejour">
        <comment>id sejour</comment>
      </column>
    </property>
    <property name="hoslabDateResultat" type="date">
      <column default="NULL" name="hoslab_date_resultat">
        <comment>date des resultats</comment>
      </column>
    </property>
    <property name="hoslabDatePrelevement" type="date">
      <column default="NULL" name="hoslab_date_prelevement">
        <comment>date du prelevement</comment>
      </column>
    </property>
    <property name="hoslabLieuPrelevement" type="integer">
      <column default="0" length="1" name="hoslab_lieu_prelevement">
        <comment>0=non renseigne 1=domicile 2=laboratoire 3=unite de soin</comment>
      </column>
    </property>
    <property name="hoslabPartenaire" type="integer">
      <column default="0" length="1" name="hoslab_partenaire">
        <comment>0=non renseigne 1=avec partenaire 2=avec plusieurs partenaires 3=sans partenaire</comment>
      </column>
    </property>
    <property name="hoslabDateRegles" type="date">
      <column default="NULL" name="hoslab_date_regles">
        <comment>date des dernieres regles</comment>
      </column>
    </property>
    <property name="hoslabAnonyme" type="boolean">
      <column default="false" name="hoslab_anomyme">
        <comment>si true alors anonyme</comment>
      </column>
    </property>
    <property name="hoslabGrossesse" type="boolean">
      <column default="false" name="hoslab_grossesse">
        <comment>grossesse oui ou non</comment>
      </column>
    </property>
    <property name="hoslabDiabete" type="boolean">
      <column default="false" name="hoslab_diabete">
        <comment>diabete oui ou non</comment>
      </column>
    </property>
    <property name="hoslabImmunodepressif" type="boolean">
      <column default="false" name="hoslab_immunodepressif">
        <comment>immunodepressif oui ou non</comment>
      </column>
    </property>
    <property name="hoslabTraitement" type="boolean">
      <column default="false" name="hoslab_traitement">
        <comment>traitement oui ou non</comment>
      </column>
    </property>
    <property name="hoslabLequel" type="string">
      <column length="50" name="hoslab_lequel">
        <comment>quel traitement</comment>
      </column>
    </property>
    <property name="hoslabUrgent" type="integer">
      <column default="0" name="hoslab_urgent">
        <comment>urgent 0=normal 1=un peu urgent 2=tres urgent 3=instannee</comment>
      </column>
    </property>
    <property name="hoslabRegPatient" type="double">
      <column default="0" name="hoslab_reg_patient">
        <comment>reglement patient</comment>
      </column>
    </property>
    <property name="hoslabRegTiers" type="double">
      <column default="0" name="hoslab_reg_tiers">
        <comment>reglement tous tiers</comment>
      </column>
    </property>
    <many-to-one class="com.epegase.systeme.classe.HospitalisationEntete" column="hos_id" lazy="false" name="HospitalisationEntete" not-null="true"/>
  </class>
</hibernate-mapping>
